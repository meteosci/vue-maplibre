{
  "name": "vue-maplibre",
  "packageManager": "pnpm@6.30.1",
  "workspaces": [
    "packages/*",
    "play",
    "docs",
    "demo"
  ],
  "description": "A Vue 3 Component Library for maplibre-gl",
  "scripts": {
    "cz": "czg",
    "test": "jest --coverage && coveralls < coverage/lcov.info",
    "gen": "bash ./scripts/gc.sh",
    "gen:version": "sucrase-node scripts/gen-version.ts",
    "update:version": "sucrase-node scripts/update-version.ts",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build": "gulp --require sucrase/register/ts -f build/gulpfile.ts",
    "build:theme": "pnpm run build -C packages/theme-default",
    "build:indices": "sucrase-node build/indices.ts",
    "format": "prettier --write .",
    "lint": "eslint ./packages ./demo --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "website-build": "rimraf website-dist && cross-env NODE_ENV=production webpack --config ./website/webpack.config.js",
    "website-dev": "webpack-dev-server --config ./website/webpack.config.js",
    "upgrade:vue": "bash ./scripts/update-versions.sh",
    "postinstall": "pnpm gen:version",
    "preinstall": "npx only-allow pnpm -y",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "license": "MIT",
  "homepage": "https://meteosci.com/vue-maplibre",
  "repository": {
    "type": "git",
    "url": "git://github.com/meteosci/vue-maplibre.git"
  },
  "bugs": {
    "url": "https://github.com/meteosci/vue-maplibre/issues"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@meteosci/vue-maplibre": "workspace:*",
    "@types/three": "^0.163.0",
    "@vue-maplibre/components": "workspace:*",
    "@vue-maplibre/composables": "workspace:*",
    "@vue-maplibre/demo": "workspace:*",
    "@vue-maplibre/directives": "workspace:*",
    "@vue-maplibre/locale": "workspace:*",
    "@vue-maplibre/shared": "workspace:*",
    "@vue-maplibre/theme-default": "workspace:*",
    "@vue-maplibre/utils": "workspace:*",
    "gl-matrix": "^3.4.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "maplibre-gl": "^4.1.3",
    "mitt": "^3.0.1",
    "three": "^0.163.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/logger": "^5.0.0",
    "@pnpm/types": "^10.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@sucrase/jest-plugin": "^3.0.0",
    "@types/fs-extra": "9.0.13",
    "@types/gulp": "^4.0.17",
    "@types/gulp-debug": "^2.0.36",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.7",
    "@types/undertaker": "^1.2.11",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vue/compiler-sfc": "^3.4.23",
    "algoliasearch": "^4.23.3",
    "chalk": "4.1.2",
    "commitizen": "^4.3.0",
    "components-helper": "^2.2.0",
    "cz-git": "^1.9.1",
    "czg": "^1.9.1",
    "esbuild": "^0.20.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.25.0",
    "fast-glob": "3.3.2",
    "fs-extra": "10.0.0",
    "gulp": "4.0.2",
    "gulp-debug": "^5.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.14.3",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "sucrase": "^3.35.0",
    "transliteration": "^2.3.5",
    "ts-jest": "^29.1.2",
    "ts-morph": "^22.0.0",
    "type-fest": "^4.15.0",
    "typescript": "^5.4.5",
    "vitepress": "^1.1.3",
    "vitest": "^1.5.0",
    "vue": "^3.4.23",
    "vue-jest": "5.0.0-alpha.10",
    "vue-tsc": "^2.0.13"
  }
}
