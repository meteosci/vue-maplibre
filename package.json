{
  "name": "vue-maplibre",
  "packageManager": "pnpm@9.0.4",
  "type": "module",
  "workspaces": [
    "packages/*",
    "play",
    "docs",
    "internal/*",
    "demo"
  ],
  "description": "A Vue 3 Component Library for maplibre-gl",
  "scripts": {
    "cz": "czg",
    "test": "jest --coverage && coveralls < coverage/lcov.info",
    "gen": "bash ./scripts/gc.sh",
    "gen:version": "tsx internal/build/gen-version.ts",
    "update:version": "tsx internal/build/update-version.ts",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build": "pnpm run -C internal/build start",
    "build:theme": "pnpm run -C packages/theme-default build",
    "build:indices": "tsx build/indices.ts",
    "format": "prettier --write .",
    "lint:eslint": "eslint --fix ./packages",
    "lint:prettier": "prettier --write packages/",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "stub": "pnpm run -r --parallel stub",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm -y",
    "postinstall": "pnpm stub && concurrently \"pnpm gen:version\" \"pnpm run -C internal/metadata dev\""
  },
  "license": "MIT",
  "homepage": "https://vue-maplibre.meteosci.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/meteosci/vue-maplibre.git"
  },
  "bugs": {
    "url": "https://github.com/meteosci/vue-maplibre/issues"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "dependencies": {
    "@meteosci/vue-maplibre": "workspace:*",
    "@vue-maplibre/components": "workspace:*",
    "@vue-maplibre/composables": "workspace:*",
    "@vue-maplibre/demo": "workspace:*",
    "@vue-maplibre/directives": "workspace:*",
    "@vue-maplibre/docs": "workspace:*",
    "@vue-maplibre/locale": "workspace:*",
    "@vue-maplibre/shared": "workspace:*",
    "@vue-maplibre/theme-default": "workspace:*",
    "@vue-maplibre/utils": "workspace:*",
    "gl-matrix": "^3.4.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "maplibre-gl": "^5.3.0",
    "mitt": "^3.0.1",
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/logger": "^1000.0.0",
    "@pnpm/types": "^1000.3.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.17",
    "@types/gulp-debug": "^2.0.36",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.16",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "~20.12.7",
    "@types/three": "^0.175.0",
    "@types/undertaker": "^1.2.11",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue-maplibre/build": "workspace:*",
    "@vue/compiler-sfc": "^3.5.13",
    "algoliasearch": "^5.23.0",
    "chalk": "^5.4.1",
    "commitizen": "^4.3.1",
    "components-helper": "^2.2.0",
    "concurrently": "^9.1.2",
    "consola": "^3.4.2",
    "cz-git": "^1.11.1",
    "czg": "^1.11.1",
    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.9.3",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-vue": "^10.0.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "globals": "^16.0.0",
    "gulp": "^5.0.0",
    "gulp-cli": "^3.0.0",
    "gulp-debug": "^5.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.37.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-filesize": "^10.0.0",
    "transliteration": "^2.3.5",
    "ts-jest": "^29.3.0",
    "ts-morph": "^25.0.1",
    "tsx": "^4.19.3",
    "type-fest": "^4.38.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0",
    "unplugin-vue-macros": "^2.14.5",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.9",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^9.4.2",
    "vue-jest": "5.0.0-alpha.10",
    "vue-tsc": "^2.2.8"
  }
}
